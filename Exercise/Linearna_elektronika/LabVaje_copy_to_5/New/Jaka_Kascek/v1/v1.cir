    tran 1u 5m
    
    let id = -i(vdd)
    let vds = v(3,4)
    let vgs = v(1,4)
    let vout = v(3)
    
    plot v(1) vout xlabel 'time' ylabel 'r:vin[t] g:vout[t]' 
    
    
        *fourier - popacenje
        *fourier f0 signal
    fourier 1k vout
    
        *racun ojacanja
    let vinPP = max(v(1))-min(v(1))
    let voutPP = max(v(3))-min(v(3))
    let avABS = voutPP/vinPP
    print avABS
    
    
    
    
    
